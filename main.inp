$ Include Mask
MASK IN.FILE =mask.tl1 PRINT

$ Background Doping
INITIALIZE BORON=1E15


DEPOSIT PHOTORESIST THICKNESS = 4
EXPOSE MASK=NMASK
DEVELOP
IMPLANT ARSENIC DOSE=7E12 ENERGY=220 TILT=10 ROTATION=20
ETCH PHOTORESIST ALL

DEPOSIT PHOTORESIST THICKNESS = 4
EXPOSE MASK=PMASK
DEVELOP
IMPLANT BORON DOSE=1.2E13 ENERGY=220 TILT=10 ROTATION=20
ETCH PHOTORESIST ALL

DEPOSIT PHOTORESIST THICKNESS = 4
EXPOSE MASK=PPMASK
DEVELOP
IMPLANT BORON DOSE=1E17 ENERGY=100 TILT=10 ROTATION=20
ETCH PHOTORESIST ALL

DEPOSIT PHOTORESIST THICKNESS = 4
EXPOSE MASK=NNMASK
DEVELOP
IMPLANT ARSENIC DOSE=1E16 ENERGY=220 TILT=10 ROTATION=20
ETCH PHOTORESIST ALL

$ Grow buried layer masking oxide
$DIFFUSION TEMP=1000 TIME=120 STEAM
SAVEFILE OUT.FILE=wafer.tif tif

DEPOSIT PHOTORESIST THICK=1.0
EXPOSE MASK=Oxide
DEVELOP
ETCH OXIDE THICK=1
ETCH PHOTORESIST ALL

DEPOSIT PHOTORESIST THICK=1.0
EXPOSE MASK=MOS
DEVELOP
ETCH OXIDE THICK=0.6
ETCH PHOTORESIST ALL

DEPOSIT PHOTORESIST THICK=1.0
EXPOSE MASK=MOSGATE
DEVELOP
ETCH OXIDE THICK=0.6
ETCH PHOTORESIST ALL

$Removing enough oxide for gate

$ Define gate oxide
MATERIAL MAT=POLY ^POLYCRYS
$DEPOSIT OXIDE THICK=0.1
DEPOSIT PHOTORESIST THICK=1.0
EXPOSE MASK=Poly
DEVELOP
$ETCH OXIDE THICK=0.1
ETCH PHOTORESIST ALL

$ Define polysilicon gate
DEPOSIT POLYSILICON THICK=0.1
DEPOSIT PHOTORESIST THICK=1.0
EXPOSE MASK=Poly
DEVELOP
ETCH POLYSILICON TRAP THICK=1
ETCH PHOTORESIST ALL

$ Oxidize the polysilicon gate
DIFFUSION TIME=30 TEMP=1000 INERT

DEPOSIT ALUMINUM THICK=5
ETCH ALUMINUM THICK=5
DEPOSIT ALUMINUM THICK=5
ETCH ALUMINUM THICK=5

DEPOSIT PHOTORESIST THICK=1.0
EXPOSE MASK=Metal
DEVELOP

ETCH ALUMINUM THICK=1.0
ETCH PHOTORESIST ALL

ELECTRICAL

DEPOSIT PHOTORESIST THICK=0.5
EXPOSE MASK=Metal
DEVELOP
DEPOSIT OXIDE THICK=2
ETCH OXIDE THICK = 2.5
ETCH PHOTORESIST ALL

$Not NECESSARY
$DEPOSIT PHOTORESIST NEGATIVE THICK=1.0
$EXPOSE MASK=ALLDEVICE
$DEVELOP
$ETCH POLYSILICON TRAP THICK=0.7 ANGLE=79
$ETCH OXIDE THICK=1
$ETCH PHOTORESIST ALL
$DEPOSIT OXIDE THICK=0.6504

$ Plot structure
SELECT Z=LOG10(BORON) TITLE="Etching"
PLOT.2D SCALE Y.MAX=1 Y.MIN=-3
$ Color fill the regions
COLOR SILICON COLOR=7
COLOR POLY COLOR=6
COLOR OXIDE COLOR=5
COLOR ALUM COLOR=2

ELECTRICAL X=1 THRESHOLD NMOS V="0 2 0.1"
ELECTRICAL X=2 THRESHOLD NMOS V="0 2 0.1"
ELECTRICAL X=6 THRESHOLD NMOS V="0 2 0.1"

$SAVEFILE OUT.FILE=wafer.tif tif